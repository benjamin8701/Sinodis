    <validationRules>
        <fullName>CN_Check_Create_Promotion_Member</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  ISNEW(),
  ISPICKVAL( CN_Promotion__r.CN_Status__c , &quot;Aborted&quot;) 
)</errorConditionFormula>
        <errorMessage>You are not allowed to add new promotion member while promotion is aborted.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Prevent_Change_Account</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
ISCHANGED( CN_Account__c ),
NOT(ISNEW())
)</errorConditionFormula>
        <errorDisplayField>CN_Account__c</errorDisplayField>
        <errorMessage>It&apos;s not allow change Account.</errorMessage>
    </validationRules>
    <validationRules>
