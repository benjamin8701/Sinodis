    <validationRules>
        <fullName>CN_QTP_ChecK_Related_Account</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
 CN_Quote__r.Opportunity.Account.Id !=  CN_Promotion_Member__r.CN_Account__r.Id 
)</errorConditionFormula>
        <errorDisplayField>CN_Promotion_Member__c</errorDisplayField>
        <errorMessage>The Promotion selected should be same as Quote&apos;s Account.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_QTP_Check_Promotion_Duration</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
ISNEW(),
NOT( CN_Promotion_Member__r.CN_Promotion_Start_Date__c &lt;= CN_Quote__r.CN_Effective_Date__c &amp;&amp;  CN_Quote__r.ExpirationDate  &lt;=  CN_Promotion_Member__r.CN_Promotion_End_Date__c )
)</errorConditionFormula>
        <errorDisplayField>CN_Promotion_Member__c</errorDisplayField>
        <errorMessage>The Promotion is not in activate duration.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_QTP_Prevent_Edit</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  ISCHANGED(CN_Promotion_Member__c)
)</errorConditionFormula>
        <errorMessage>You are not allow to edit the quote to promotion.</errorMessage>
    </validationRules>
