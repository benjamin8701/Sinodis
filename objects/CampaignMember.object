        <fullName>CN_Prevent_NotOwner_Creation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  ISNEW(),
  NOT( Lead.CN_Is_From_Web_To_Lead__c ),
  Campaign.OwnerId &lt;&gt;  $User.Id 
