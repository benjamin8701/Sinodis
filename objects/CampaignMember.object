    <fields>
        <fullName>AccountId</fullName>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>CN_Able_to_generate_opportunity__c</fullName>
        <externalId>false</externalId>
        <formula>AND( 
  OR(CN_Type__c = &apos;Account&apos;,CN_Type__c = &apos;Contact&apos;),
  ISPICKVAL( Status , &apos;Attended&apos;)
)</formula>
        <label>Able to generate opportunity</label>
        <type>Checkbox</type>
    </fields>
    <fields>
