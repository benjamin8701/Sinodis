    <fields>
        <fullName>CN_Finance_Process_Status__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
	ISPICKVAL(Status, &quot;Approved&quot;),
	IF(
		ISBLANK(CN_Customer_Code__c ),
		$Label.CN_Quote_Finance_Process_Status_PendingOpenAccount,
		IF(
			CN_Is_Finance_Notified__c,
			IF(
				CN_Finance_Acceptance_Item_Count__c = CN_Sync_Success_Item_Count__c &amp;&amp; CN_Accepted_Item_Num__c == (CN_Finance_Acceptance_Item_Count__c + CN_Finance_Deined_Item_Count__c) &amp;&amp; CN_Accepted_Item_Num__c != 0,
				$Label.CN_Quote_Finance_Process_Status_Done + TEXT(CN_Finance_Acceptance_Item_Count__c ),
				$Label.CN_Quote_Finance_Process_Status_Processing + TEXT( CN_Accepted_Item_Num__c ) + $Label.CN_Quote_Finance_Process_Status_Finance_Accept + TEXT( CN_Finance_Acceptance_Item_Count__c ) + $Label.CN_Quote_Finance_Process_Status_Synced + TEXT( CN_Sync_Success_Item_Count__c ) + $Label.CN_Quote_Finance_Process_Status_Failed  + TEXT( CN_Sync_Failed_Item_Count__c )
			),
			$Label.CN_Quote_Finance_Process_Status_WaitingNotufy
		)
	),
	&quot;&quot;
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Finance Process Status</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
