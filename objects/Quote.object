    <fields>
        <fullName>CN_Is_Finance_Process_Done__c</fullName>
        <externalId>false</externalId>
        <formula>IF( 
 ISPICKVAL(Status, &quot;Approved&quot;) &amp;&amp;  CN_Is_Finance_Notified__c ,
 IF(
    CN_Finance_Acceptance_Item_Count__c ==  (CN_Sync_Success_Item_Count__c +  CN_Sync_Failed_Item_Count__c ) &amp;&amp; CN_Accepted_Item_Num__c == CN_Finance_Acceptance_Item_Count__c &amp;&amp; CN_Accepted_Item_Num__c != 0,
    TRUE,
    FALSE
 )
 ,FALSE
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Is Finance Process Done</label>
        <type>Checkbox</type>
