    <validationRules>
        <fullName>CN_Prevent_Edit_Ready</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
AND(
ISPICKVAL(Status , &quot;Ready for Approval&quot;),
NOT(ISCHANGED(Status ))
)
)</errorConditionFormula>
        <errorMessage>It&apos;s not allowed to edit Quote when it&apos;s Ready for Approval.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Quote_Check_EffectiveExpirationDate</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  RecordType.DeveloperName = &apos;CN_Quote&apos;,
  CN_Effective_Date__c &gt; ExpirationDate
)</errorConditionFormula>
        <errorMessage>Expiration Date should be later than Effective Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Quote_Check_Effective_Date</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  RecordType.DeveloperName = &apos;CN_Quote&apos;,
  NOT(ISCHANGED(TotalPrice)),
  ISNEW() || ISCHANGED(CN_Effective_Date__c ),
  CN_Effective_Date__c &lt;  TODAY()
)</errorConditionFormula>
        <errorDisplayField>CN_Effective_Date__c</errorDisplayField>
        <errorMessage>Effective Date shouldn&apos;t be earlier than today.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Quote_Check_Opp_Stage</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  RecordType.DeveloperName = &apos;CN_Quote_New&apos;,
  ISNEW(),
  NOT(ISPICKVAL(Opportunity.StageName , &apos;Quote/Negotiation&apos;))
)</errorConditionFormula>
        <errorDisplayField>OpportunityId</errorDisplayField>
        <errorMessage>You can&apos;t create new quote for the opportunity whose stage is not Quote/Negotiation.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Quote_Check_Prices</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  RecordType.DeveloperName = &apos;CN_Quote&apos;,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  AND( ISBLANK( CN_Approver_1__c )  )
)</errorConditionFormula>
        <errorMessage>第一个审批人不能为空</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Quote_Check_Ready_LineItem_Num</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
 ISPICKVAL(Status  , &apos;Ready for Approval&apos;) ,
 CN_Item_Num__c = 0
)</errorConditionFormula>
        <errorMessage>Please add quote line items before ready for approve.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Quote_Prevent_Edit_Approved</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  RecordType.DeveloperName = &apos;CN_Quote&apos;,
  OR(
    ISPICKVAL(Status, &quot;Approved&quot;),
    ISPICKVAL(PRIORVALUE(Status), &quot;Approved&quot;)
  ),
  OR(
    ISCHANGED(Status),
    ISCHANGED(Name),
    ISCHANGED(CN_Effective_Date__c),
    ISCHANGED(ExpirationDate)
  )
)</errorConditionFormula>
        <errorMessage>You are not allowed to edit when quote is marked as Approved.</errorMessage>
    </validationRules>
