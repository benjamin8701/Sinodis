    <validationRules>
        <fullName>CN_AccountApproved_NotSynced</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  RecordType.DeveloperName = &apos;CN_Credit_Term_Application&apos;,
  ISPICKVAL(CN_Account__r.CN_Account_Status__c , &quot;Approved&quot;),
  NOT(ISPICKVAL(CN_Account__r.CN_Sync_Status__c ,&quot;Success&quot;)) 
)</errorConditionFormula>
        <errorMessage>Please wait for a few minutes until the account synced to SAP.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_CTA_Prevent_Change_Status_In_Approve</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  RecordType.DeveloperName = &apos;CN_Credit_Term_Application&apos;,
  IF(CN_Is_Approving__c ,  ISCHANGED( CN_Status__c ) , FALSE)
)</errorConditionFormula>
        <errorMessage>Not Allowed to Change Status in Approving.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Check_PaymentTerm</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  RecordType.DeveloperName = &apos;CN_Credit_Term_Application&apos;,
  OR(
    ISPICKVAL(CN_Payment_Term_Negotiated__c, &quot;CIA&quot;),
    ISPICKVAL(CN_Payment_Term_Negotiated__c, &quot;D000&quot;)
  ),
  ISPICKVAL( CN_Reference_Day__c ,&quot;Fapiao_Day&quot;)
)</errorConditionFormula>
        <errorDisplayField>CN_Reference_Day__c</errorDisplayField>
        <errorMessage>You are not allowed to choose Fapiao Day for CIA or COD customer.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Prevent_Create_Or_Change</fullName>
        <active>true</active>
        <description>If have ongoing CTA, not allow to Create or change status Edit (Status from Cancal to New)

Ongoning CTA means Account.Latest CTA Status is  not Blank and  approved</description>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  RecordType.DeveloperName = &apos;CN_Credit_Term_Application&apos;,
  OR(
     AND(
       ISPICKVAL(CN_Status__c,&quot;New&quot;),
       ISPICKVAL(PRIORVALUE(CN_Status__c),&quot;Cancel&quot; )
     ),
     ISNEW() 
  ),
  NOT(
    OR(
      ISPICKVAL( CN_Account__r.CN_Latest_Credit_Term_App_Status__c , &quot;Approved&quot;),
      ISBLANK(TEXT(CN_Account__r.CN_Latest_Credit_Term_App_Status__c))
    )
  )
  
)</errorConditionFormula>
        <errorMessage>Please continue with the ongoing credit term application or cancel it and create a new one.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Prevent_Edit_For_Ready_Approved</fullName>
        <active>true</active>
        <description>CN: Can&apos;t edit except Status when status is Ready for Approval and can&apos;t edit anything when status is Approved</description>
        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
RecordType.DeveloperName = &apos;CN_Credit_Term_Application&apos;,
OR(AND(ISPICKVAL(CN_Status__c, &quot;Ready for Approval&quot;),   NOT(ISCHANGED(CN_Status__c))), AND(ISPICKVAL(PRIORVALUE(CN_Status__c),&quot;Approved&quot;), ISPICKVAL(CN_Status__c, &quot;Approved&quot;)))
)</errorConditionFormula>
        <errorMessage>You can&apos;t change this Credit Term Application when Status is Ready for Approval or Approved.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Prevent_Edit_On_Ready_And_Submit</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  RecordType.DeveloperName = &apos;CN_Credit_Term_Application&apos;,
  OR(
    ISPICKVAL( CN_Account__r.CN_Account_Status__c , &quot;Ready for Approval&quot;),
    ISPICKVAL( CN_Account__r.CN_Account_Status__c , &quot;Submitted for Approval&quot;)
  ),
  ISNEW()
)</errorConditionFormula>
        <errorMessage>You are not allowed to create credit term application when account is already marked as ready for approval or Submitted for Approval.</errorMessage>
    </validationRules>
