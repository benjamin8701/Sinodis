    <validationRules>
        <fullName>CN_PPBE_Channel_Check_Field</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  NOT(ISPICKVAL(CN_Channel__c,&apos;LP&apos;)),
  OR(
    CN_UnitPrice__c = 0,
    ISBLANK(CN_UnitPrice__c)
  )
)</errorConditionFormula>
        <errorDisplayField>CN_UnitPrice__c</errorDisplayField>
        <errorMessage>Please input Channel Price.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_PPBE_Channel_Prevent_ListPrice</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  NOT(ISPICKVAL(CN_Channel__c,&apos;LP&apos;)),
  NOT(ISBLANK( CN_Standard_List_Price__c ))
)</errorConditionFormula>
        <errorDisplayField>CN_Standard_List_Price__c</errorDisplayField>
        <errorMessage>Please don&apos;t input the List Price for Channel Preset Price Book Entry</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_PPBE_Check_Effective_Date</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  OR(ISCHANGED(CN_Effective_Date__c ),ISNEW()),
  CN_Effective_Date__c &lt;= TODAY()
)</errorConditionFormula>
        <errorDisplayField>CN_Effective_Date__c</errorDisplayField>
        <errorMessage>Effective Date must be later than today</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_PPBE_Check_ExpirationDate</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  RecordType.DeveloperName = &apos;CN_Preset_PriceBook_Entry&apos;,
  CN_Effective_Date__c &gt; CN_Expiration_Date__c
)</errorConditionFormula>
        <errorDisplayField>CN_Expiration_Date__c</errorDisplayField>
        <errorMessage>Expiration Date should be later than Effective Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_PPBE_Prevent_Edit</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  OR(CN_Is_Effected__c = TRUE, CN_Effective_Date__c &lt;= Today()),
  OR(
    ISCHANGED(CN_Channel__c),
    ISCHANGED(CN_UnitPrice__c),
    ISCHANGED(CN_Discount_Price__c),
    ISCHANGED(CN_Effective_Date__c),
    ISCHANGED(CN_Expiration_Date__c),
    ISCHANGED(CN_Standard_List_Price__c),
    ISCHANGED(CN_Max_Discount_Price__c),
    ISCHANGED(CN_Product__c)
  )
)</errorConditionFormula>
        <errorMessage>You are not allowed to edit the Preset Price Book Entry which has been effected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_PPBE_Standard_Check_Field</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  ISPICKVAL(CN_Channel__c,&apos;LP&apos;),
  OR(
    NOT(ISBLANK(CN_Discount_Price__c )),
    NOT(ISBLANK(CN_Max_Discount_Price__c)),
    NOT(ISBLANK(CN_UnitPrice__c))
  )
)</errorConditionFormula>
        <errorMessage>Please don&apos;t input the Channel ,Discount Price and Max Discount Price for Preset Price Book Entry which channel is &apos;LP&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_PPBE_Standard_Check_ListPrice</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  ISPICKVAL(CN_Channel__c,&apos;LP&apos;),
  ISBLANK( CN_Standard_List_Price__c )
)</errorConditionFormula>
        <errorDisplayField>CN_Standard_List_Price__c</errorDisplayField>
        <errorMessage>Please input the list Price for Preset Price Book Entry which channel is &apos;LP&apos;</errorMessage>
    </validationRules>
