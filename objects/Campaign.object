    <validationRules>
        <fullName>CN_Campaign_Audiences_Number_IsBlank</fullName>
        <active>false</active>
        <errorConditionFormula>AND($Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c, OR(RecordType.DeveloperName = &apos;CN_Campaign&apos;, RecordType.DeveloperName = &apos;CN_Parent_Campaign&apos;),  ISPICKVAL(Status, &quot;Completed&quot;),  ISBLANK(CN_Number_of_Audiences__c))</errorConditionFormula>
        <errorDisplayField>CN_Number_of_Audiences__c</errorDisplayField>
        <errorMessage>Number of audiences is a must field to input for livestreaming campaign when campaign completes.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Campaign_Check_Chef_Manage</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  $Profile.Name = &apos;CN Chef Manager&apos;,
  OR(
    RecordType.DeveloperName = &apos;CN_Campaign&apos;,
    RecordType.DeveloperName = &apos;CN_Parent_Campaign&apos;
  ),
  NOT(CN_VIP__c)
)</errorConditionFormula>
        <errorMessage>You are only allow to create or edit the vip campaign</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Campaign_Check_Customer_Category</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c, 
  OR(
    RecordType.DeveloperName = &apos;CN_Campaign&apos;, 
    RecordType.DeveloperName = &apos;CN_Parent_Campaign&apos;
  ),
  ISPICKVAL(Type, &apos;Road Show&apos;),
  ISNULL(CN_Customer_Category__c ) 
)</errorConditionFormula>
        <errorDisplayField>CN_Customer_Category__c</errorDisplayField>
        <errorMessage>Customer category is mandatory to choose when event type of campaign is road show.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Campaign_Check_Start_Date</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c, 
  OR(
    RecordType.DeveloperName = &apos;CN_Campaign&apos;, 
    RecordType.DeveloperName = &apos;CN_Parent_Campaign&apos;
  ),
  OR(
    ISNEW(),
    ISCHANGED(StartDate)
  ),  
  StartDate &lt; TODAY()
)</errorConditionFormula>
        <errorDisplayField>StartDate</errorDisplayField>
        <errorMessage>Start date must not be earlier than today</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Campaign_Check_numOfAud_LiveStreaming</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c, 
  OR(
    RecordType.DeveloperName = &apos;CN_Campaign&apos;, 
    RecordType.DeveloperName = &apos;CN_Parent_Campaign&apos;
  ),  
  INCLUDES(CN_Execution_Type__c, &quot;Livestreaming&quot;) ,
  ISPICKVAL(Status, &quot;Completed&quot;),
  ISBLANK( CN_Number_of_Audiences__c )
)</errorConditionFormula>
        <errorDisplayField>CN_Number_of_Audiences__c</errorDisplayField>
        <errorMessage>You must enter number of audiences for livestreaming campaign</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Campaign_Completed_Block_Edit</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c, 
OR(
    RecordType.DeveloperName = &apos;CN_Campaign&apos;, 
    RecordType.DeveloperName = &apos;CN_Parent_Campaign&apos;
   ), 
OR(
    ISPICKVAL(PRIORVALUE(Status), &quot;Completed&quot;),
    ISPICKVAL(PRIORVALUE(Status), &quot;Aborted&quot;)
   )
)</errorConditionFormula>
        <errorMessage>You cannot edit anything once completed or aborted.</errorMessage>
    </validationRules>
