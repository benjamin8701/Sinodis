    <validationRules>
        <fullName>CN_Event_Check_Status</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  RecordType.DeveloperName == &apos;CN_Visit&apos;,
  ISPICKVAL( CN_Visit_Status__c , &apos;Completed&apos;),
  ISBLANK(  CN_Meeting_MInutes__c ) 
)</errorConditionFormula>
        <errorDisplayField>CN_Meeting_MInutes__c</errorDisplayField>
        <errorMessage>You must enter the Meeting Minutes when complete the visit.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Event_Check_Status_Cancelled</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  RecordType.DeveloperName == &apos;CN_Visit&apos;,
  ISPICKVAL( CN_Visit_Status__c, &apos;Cancelled&apos;),
  ISPICKVAL( PRIORVALUE(CN_Visit_Status__c),&apos;Completed&apos;)
)</errorConditionFormula>
        <errorMessage>You are not allowed to cannel the visit after it has been completed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Event_Prevent_Creation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  OR(RecordType.DeveloperName == &apos;CN_Visit&apos;,RecordType.DeveloperName == &apos;CN_Event&apos;),
  ISNEW(),
  $Profile.Name = &quot;CN Sales Lead&quot;
)</errorConditionFormula>
        <errorMessage>You are not allowed to create Event or Visit.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Event_Prevent_Edit_ByWhen</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  RecordType.DeveloperName == &apos;CN_Visit&apos;,
  NOT(ISBLANK( PRIORVALUE( CN_By_when__c ) )),
  ISCHANGED( CN_By_when__c ) 
)</errorConditionFormula>
        <errorDisplayField>CN_By_when__c</errorDisplayField>
        <errorMessage>You are not allow to edit the By When field again.</errorMessage>
    </validationRules>
