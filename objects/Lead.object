    <validationRules>
        <fullName>CN_Check_Change_Status_Backwards</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  NOT(ISNEW()),
  OR(
    AND(ISCHANGED(Status),ISPICKVAL(PRIORVALUE(Status), &quot;MKT Qualified&quot;),ISPICKVAL(Status , &quot;Open&quot;)),
    AND(ISCHANGED(Status),ISPICKVAL(PRIORVALUE(Status), &quot;Assigned&quot;),OR( ISPICKVAL(Status, &quot;MKT Qualified&quot;), ISPICKVAL(Status , &quot;Open&quot;)))
  )
)</errorConditionFormula>
