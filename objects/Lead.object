NOT(REGEX(CN_Zip_Postal_Code__c, &quot;\\d{6}&quot;) )
)</errorConditionFormula>
        <errorDisplayField>CN_Zip_Postal_Code__c</errorDisplayField>
        <errorMessage>Postal code must be six digits.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Check_Sales_Change_Status</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
NOT(ISNEW()),
ISCHANGED(Status ),
OR(
ISPICKVAL( Status , &quot;Open&quot;),
ISPICKVAL( Status , &quot;MKT Qualified&quot;),
ISPICKVAL( Status , &quot;Assigned&quot;)
),
OR(
$Profile.Name = &apos;CN Sales&apos;,
$Profile.Name = &apos;CN Sales Manager&apos;,
$Profile.Name = &apos;CN Sales Lead&apos;
)
)</errorConditionFormula>
        <errorDisplayField>Status</errorDisplayField>
        <errorMessage>It&apos;s not allow to select previous status</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Check_Sales_Create_Status</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
 ISNEW(),
 NOT(ISPICKVAL( Status , &quot;Open&quot;)),
 OR(
   $Profile.Name = &apos;CN Sales&apos;,
   $Profile.Name = &apos;CN Sales Manager&apos;,
   $Profile.Name = &apos;CN Sales Lead&apos;
 )
)</errorConditionFormula>
        <errorDisplayField>Status</errorDisplayField>
        <errorMessage>Please keep Lead status as open</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Prevent_Edit_New_Contact</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
(ISPICKVAL( PRIORVALUE(Status ) , &quot;Closed - Converted&quot;) || ISPICKVAL(Status , &quot;Closed - Converted&quot;)),
 NOT(ISBLANK( CN_Converted_Contact_Id_From_CR__c )),
 NOT(ISCHANGED(CN_Converted_Contact_Id_From_CR__c ))
)</errorConditionFormula>
        <errorMessage>This Lead had been converted into contact.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Prevent_Eidt_When_Dead</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
OR(
 ISCHANGED(Status ) &amp;&amp; ISPICKVAL( PRIORVALUE(Status) , &quot;Closed - Dead&quot;) ,
 NOT(ISCHANGED(Status )) &amp;&amp; ISPICKVAL( Status , &quot;Closed - Dead&quot;) 
)
)</errorConditionFormula>
        <errorDisplayField>Status</errorDisplayField>
        <errorMessage>It&apos;s not allow to edit after lead dead.</errorMessage>
    </validationRules>
