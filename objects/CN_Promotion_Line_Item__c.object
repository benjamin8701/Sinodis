    <validationRules>
        <fullName>CN_Check_Price</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
 CN_Promotion_Price__c &lt;= 0
)</errorConditionFormula>
        <errorDisplayField>CN_Promotion_Price__c</errorDisplayField>
        <errorMessage>Promotion Price must greater than 0</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Check_Product_Active</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
ISCHANGED(CN_Product__c),
NOT( CN_Product__r.IsActive )
)</errorConditionFormula>
        <errorDisplayField>CN_Product__c</errorDisplayField>
        <errorMessage>Product is inactive.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Prevent_Create_New_Item</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
 NOT(CN_Is_Promotion_Planned__c ),
 ISNEW()
)</errorConditionFormula>
        <errorMessage>It&apos;s not allow to create new promotion line item if promotion is not planned</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Prevent_Edit_Item</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
 NOT(CN_Is_Promotion_Planned__c)
)</errorConditionFormula>
        <errorMessage>It&apos;s not allowed to edit if promotion is not planned.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
