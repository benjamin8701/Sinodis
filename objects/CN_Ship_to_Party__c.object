    <validationRules>
        <fullName>CN_Check_Account_NotNull</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  RecordType.DeveloperName = &apos;CN_Ship_to_Party&apos;,
  ISBLANK(CN_Sold_To_Party_Customer__c) 
)</errorConditionFormula>
        <errorDisplayField>CN_Sold_To_Party_Customer__c</errorDisplayField>
        <errorMessage>Ship-to must have an associated account.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Check_ContactName</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  RecordType.DeveloperName = &apos;CN_Ship_to_Party&apos;,
  OR(
    AND(  ISCHANGED(CN_Contact_Name__c ),LEN( CN_Contact_Name__c ) &lt; 2),
    AND(  ISNEW() , NOT(ISBLANK(CN_Contact_Name__c )), LEN( CN_Contact_Name__c ) &lt; 2)
  )
)</errorConditionFormula>
        <errorDisplayField>CN_Contact_Name__c</errorDisplayField>
        <errorMessage>Contact Name must be entered more than two words.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Check_Moblie_NumberFormat</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(ISBLANK( CN_Mobile_Phone__c )),
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  NOT(OR(REGEX(CN_Mobile_Phone__c , &apos;1\\d{10}&apos;),REGEX(CN_Mobile_Phone__c ,&apos;\\d{3,4}-\\d{8}(-\\d{2,5})?&apos;)))
)</errorConditionFormula>
        <errorDisplayField>CN_Mobile_Phone__c</errorDisplayField>
        <errorMessage>Please input a phone number with a valid format.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Check_Telephone_NumberFormat</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(ISBLANK( CN_Telephone__c )),
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  NOT(OR(REGEX(CN_Telephone__c , &apos;1\\d{10}&apos;),REGEX(CN_Telephone__c ,&apos;\\d{3,4}-\\d{8}(-\\d{2,5})?&apos;)))
)</errorConditionFormula>
        <errorDisplayField>CN_Telephone__c</errorDisplayField>
        <errorMessage>Please input a phone number with a valid format.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Prevent_CreateOrEdit</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  NOT(ISPICKVAL( CN_Sold_To_Party_Customer__r.CN_Account_Status__c  ,&quot;New&quot;)),
  NOT(AND( ISNEW() ,  NOT(ISBLANK(CN_Change_Request__c)))),
  NOT(ISCHANGED(CN_Change_Request__c)),
  NOT(ISCHANGED( CN_Sync_Status__c )),
  NOT(ISCHANGED(  CN_Latest_CR_Sync_Status__c ))
)</errorConditionFormula>
        <errorMessage>Please create ship-to via creating a new ship-to change request.
Please edit existing ship-to via creating a update ship-to change request.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Prevent_Edit_Synced_Success</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  BEGINS(RecordType.DeveloperName, &quot;CN&quot;),
  ISCHANGED(CN_Sync_Status__c),
  ISPICKVAL(PRIORVALUE(CN_Sync_Status__c) , &quot;Success&quot;) 
)</errorConditionFormula>
        <errorMessage>E-099 : Pls don&apos;t update sync information when the ship to party has been synchronized.</errorMessage>
    </validationRules>
