    <validationRules>
        <fullName>CN_PB_Check_Effective_Date</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  RecordType.DeveloperName = &apos;CN_Price_Book&apos;,
  OR(
    ISNEW(),
    ISCHANGED(CN_Effective_Date__c)
  ),
  CN_Effective_Date__c &lt;  TODAY()
)</errorConditionFormula>
        <errorDisplayField>CN_Effective_Date__c</errorDisplayField>
        <errorMessage>Effective Date should be later than today.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_PB_Check_ExpirationDate</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  RecordType.DeveloperName = &apos;CN_Price_Book&apos;,
  CN_Effective_Date__c &gt; CN_Expiration_Date__c
)</errorConditionFormula>
        <errorDisplayField>CN_Expiration_Date__c</errorDisplayField>
        <errorMessage>Expiration Date should be later than Effective Date.</errorMessage>
    </validationRules>
