    <validationRules>
        <fullName>CN_Prevent_Edit_After_Synced</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
 CN_Is_Line_Item_Synced__c ,
ISCHANGED(CN_Customer_Acceptance__c ) ||  ISCHANGED(  CN_Finance_Acceptance__c )
)</errorConditionFormula>
        <errorMessage>It&apos;s not allowed to change Customer Acceptance or Finance Acceptance after synced to SAP.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Prevent_Edit_Approved</fullName>
        <active>false</active>
        <description>CN_Prevent_Edit_AfterQuoteApproved</description>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  OR(
    OR(
      ISCHANGED( UnitPrice ),
      ISCHANGED( Description ),
      ISCHANGED( Quantity )
    ),
    ISNEW()
  ),
  ISPICKVAL( Quote.Status , &quot;Approved&quot;),
  NOT(CN_From_Splited__c),
  NOT(ISCHANGED(CN_Sync_Status__c)),
  NOT(ISCHANGED(CN_Sync_Time__c)),
  NOT(ISCHANGED(CN_Sync_Error_Message__c))
)</errorConditionFormula>
        <errorMessage>Pls don&apos;t update or create the quote line item when the quote has been approved.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Prevent_Edit_Finance_Accept</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
ISBLANK(TEXT(CN_Customer_Acceptance__c )),
  ISCHANGED( CN_Finance_Acceptance__c ) 
)</errorConditionFormula>
        <errorDisplayField>CN_Finance_Acceptance__c</errorDisplayField>
        <errorMessage>It&apos;s not allowed to change Finance Acceptance until Customer Acceptance had been filled.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Prevent_Edit_Ready</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
 CN_Quote_Status__c = &apos;Ready for Approval&apos; ,
NOT(ISCHANGED(CN_Latest_Ready_DateTime__c))
)</errorConditionFormula>
        <errorMessage>Please don&apos;t update or create the quote line item when the quote is ready for approval.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_QLI_Check_Reject_Only_My_Approve</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
 CN_Quote_Status__c = &apos;Submitted for Approval&apos;,
 ISCHANGED( CN_Approval_Status__c ),
 NOT(CN_Is_Need_My_Approval__c)
)</errorConditionFormula>
        <errorDisplayField>CN_Approval_Status__c</errorDisplayField>
        <errorMessage>You can only mark approval status for the Quote Line Item that required you to approve.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_QLI_Check_Sales_Price</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
    $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
    NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
    if( CN_Is_Applied_Promotion__c, UnitPrice &lt;  CN_Promotion_Price__c , false )     

)</errorConditionFormula>
        <errorMessage>Sales Price must greater or equal than Promotion Price.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_QLI_Prevent_Edit_After_Notify_Fin</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
NOT(IF($Profile.Name = &quot;CN Finance MDM&quot;, TRUE, FALSE)),
  Quote.CN_Is_Finance_Notified__c ,
 ISCHANGED( CN_Customer_Acceptance__c )
)</errorConditionFormula>
        <errorDisplayField>CN_Customer_Acceptance__c</errorDisplayField>
        <errorMessage>It&apos;s not allowed to change Customer Acceptance after notified Finance MDM.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_QLI_Prevent_Edit_Apporval_Status</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
 OR(
    CN_Quote_Status__c = &apos;Submitted for Approval&apos;,
    AND( PRIORVALUE(CN_Quote_Status__c ) = &apos;Submitted for Approval&apos;, CN_Quote_Status__c = &apos;Rejected&apos; )
 ),
 NOT(ISCHANGED( CN_Approval_Status__c ))
)</errorConditionFormula>
        <errorMessage>It&apos;s only allowed to change approval status in approval processing.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_QLI_Prevent_Edit_Approve_Complete</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
NOT(ISNEW()),

CN_Is_Approval_Completed__c,
NOT(ISCHANGED(CN_Customer_Acceptance__c ) || ISCHANGED(CN_Finance_Acceptance__c)),
NOT(ISCHANGED(CN_Is_Approval_Completed__c))
)</errorConditionFormula>
        <errorMessage>It&apos;s not allowed to edit Quote Line Item after approve complete.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_QLI_Prevent_Edit_Customer_Accpt</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
 CN_Quote_Status__c != &apos;Approved&apos;,
 ISCHANGED( CN_Customer_Acceptance__c )
)</errorConditionFormula>
        <errorDisplayField>CN_Customer_Acceptance__c</errorDisplayField>
        <errorMessage>Customer Acceptance could only be edit after Quote approved.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_QLI_Prevent_Edit_Except_Acceptance</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
NOT(ISNEW()),
 NOT(CN_Quote_Status__c != &apos;New&apos; || CN_Quote_Status__c != &apos;Rejected&apos;),
 NOT(ISCHANGED( CN_Approval_Status__c ) || ISCHANGED(CN_Customer_Acceptance__c ) || ISCHANGED(CN_Finance_Acceptance__c ))
)</errorConditionFormula>
        <errorMessage>It&apos;s not allowed to edit Quote Line Item when Quote is approving or approved.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_QLI_Prevent_Edit_Fin_Acceptance</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
ISCHANGED( CN_Finance_Acceptance__c ),
 ISPICKVAL( CN_Customer_Acceptance__c , &quot;Denied&quot;),
  ISPICKVAL( CN_Finance_Acceptance__c , &quot;Accepted&quot;)
)</errorConditionFormula>
        <errorDisplayField>CN_Finance_Acceptance__c</errorDisplayField>
        <errorMessage>It&apos;s not allowed to accept Quote Line Item which denied by customer.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_QLI_Prevent_Edit_Reject</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
 CN_Quote_Status__c = &quot;Rejected&quot;,
 ISCHANGED( CN_Approval_Status__c )
)</errorConditionFormula>
        <errorMessage>It&apos;s not allow to change approve status when Quote is rejected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Quote_Prevent_Edit_Promotion_Member</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
 CN_Quote_Status__c != &apos;New&apos;,
 ISCHANGED( CN_Promotion_Member__c ) || ISCHANGED( CN_Promotion_Price__c )
)</errorConditionFormula>
        <errorDisplayField>CN_Promotion_Member__c</errorDisplayField>
        <errorMessage>It&apos;s not allowed to apply or un-apply Promotion when Quote in approval process or approved.</errorMessage>
    </validationRules>
</CustomObject>
