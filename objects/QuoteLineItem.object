        <externalId>false</externalId>
        <formula>if(  Quote.CN_Is_Finance_Notified__c &amp;&amp; ISPICKVAL(CN_Customer_Acceptance__c,&apos;Accepted&apos;) , 

   If (  ISPICKVAL(CN_Finance_Acceptance__c , &apos;Accepted&apos; )  , 
       if ( ISPICKVAL(CN_Sync_Status__c , &apos;Success&apos;), &apos;Completed&apos;,
             if ( ISPICKVAL(CN_Sync_Status__c , &apos;Failed&apos;), &apos;Sync Failed&apos;, &apos;In progress&apos; ) 
          ),

       if ( ISPICKVAL(CN_Finance_Acceptance__c , &apos;Denied&apos;), &apos;Denied&apos;, &apos;Waitting Finance confirm&apos; )
     
    ),
   &apos;&apos;
)</formula>
