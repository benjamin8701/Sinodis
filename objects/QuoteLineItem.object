    <fields>
        <fullName>CN_Tax_Formula__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
  Quote.Opportunity.Account.CN_Tax_Classification__c == &apos;0&apos;,
  CASE( Product2.CN_Tax_Code__c , &apos;0&apos;, &apos;0&apos;, &apos;1&apos;, &apos;17&apos;,&apos;0&apos;),
  IF(
    Quote.Opportunity.Account.CN_Tax_Classification__c == &apos;1&apos;,
    CASE( Product2.CN_Tax_Code__c, &apos;0&apos;, &apos;0&apos;, &apos;1&apos;, &apos;13&apos;,&apos;2&apos;,&apos;13&apos;,&apos;5&apos;,&apos;9&apos;,&apos;0&apos;),
    &apos;0&apos;
  )
) &amp; &apos;%&apos;</formula>
        <label>Tax Formula</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
