    <validationRules>
        <fullName>CN_Prevent_Edit_Approved</fullName>
        <active>true</active>
        <description>CN_Prevent_Edit_AfterQuoteApproved</description>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  OR(
    OR(
      ISCHANGED( UnitPrice ),
      ISCHANGED( Description ),
      ISCHANGED( Quantity )
    ),
    ISNEW()
  ),
  ISPICKVAL( Quote.Status , &quot;Approved&quot;)
)</errorConditionFormula>
        <errorMessage>Pls don&apos;t update or create the quote line item when the quote has been approved.</errorMessage>
    </validationRules>
</CustomObject>
