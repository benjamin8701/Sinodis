    <validationRules>
        <fullName>CN_Case_Check_Product_Info</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  ISPICKVAL(Reason , &quot;Quality&quot;), 
  ISBLANK(ProductId)|| ISBLANK( CN_SAP_Batch_Number__c )  || ISBLANK( CN_SKU_Quantity__c )
)</errorConditionFormula>
        <errorDisplayField>Reason</errorDisplayField>
        <errorMessage>Please fill in Product, SKU Quantity and SAP Batch Number</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Check_Complaint_Date</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  BEGINS(RecordType.DeveloperName,&quot;CN&quot;),
  OR(
    ISNEW(),
    ISCHANGED(CN_Complaint_Date__c )
  ),
  CN_Complaint_Date__c &gt; TODAY()
)</errorConditionFormula>
        <errorDisplayField>CN_Complaint_Date__c</errorDisplayField>
        <errorMessage>Complaint cannot be later than today</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Complaint_Check_Reason_CS</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  BEGINS(RecordType.DeveloperName,&quot;CN&quot;),
  $Profile.Name = &quot;CN C/S&quot;,
  ISPICKVAL(CN_Resolve_Reason__c, &quot;Brand Quality&quot;)
)</errorConditionFormula>
        <errorDisplayField>CN_Resolve_Reason__c</errorDisplayField>
        <errorMessage>You can&apos;t set the Resolve Reason as &quot;Brand Quality&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Complaint_Check_Reason_QA</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  BEGINS(RecordType.DeveloperName,&quot;CN&quot;),
  $Profile.Name = &quot;CN QA&quot;,
  ISPICKVAL(CN_Resolve_Reason__c, &quot;Customer Service&quot;)
)</errorConditionFormula>
        <errorDisplayField>CN_Resolve_Reason__c</errorDisplayField>
        <errorMessage>You can&apos;t set the Resolve Reason as &quot;Customer Service&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Complaint_Check_Status_Accepted</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  BEGINS(RecordType.DeveloperName,&quot;CN&quot;),
  NOT(ISNEW()),
  ISPICKVAL( PRIORVALUE(Status) , &apos;Accepted&apos;),
  OR(
    AND(ISPICKVAL( Status, &apos;Resolved&apos;),NOT(CN_Resolved_Button__c)),
    NOT(OR(ISPICKVAL(Status, &apos;Accepted&apos;),ISPICKVAL(Status,&apos;Resolved&apos;)))
  )
)</errorConditionFormula>
        <errorDisplayField>Status</errorDisplayField>
        <errorMessage>You are not allowed to change the status manually.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Complaint_Check_Status_Assigned</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  BEGINS(RecordType.DeveloperName,&quot;CN&quot;),
  ISPICKVAL( PRIORVALUE(Status) , &apos;Assigned&apos;),
  OR(
    AND( 
      OR(ISPICKVAL( Status, &apos;Rejected&apos;),ISPICKVAL( Status, &apos;Accepted&apos;)),
      NOT(ISCHANGED(OwnerId))
    ),
    NOT(OR(
      ISPICKVAL(Status,&apos;Assigned&apos;),
      ISPICKVAL(Status,&apos;Rejected&apos;),
      ISPICKVAL(Status,&apos;Accepted&apos;),
      ISPICKVAL(Status,&apos;New&apos;)
    )),
    AND(
      ISPICKVAL(Status,&apos;New&apos;),
      IF(
        CN_Recall_Button__c,
        IF(ISCHANGED(CN_Recall_Button__c),FALSE,NOT(ISCHANGED(CN_Recall_Date__c))),
        TRUE
      )
    )
  )
)</errorConditionFormula>
        <errorDisplayField>Status</errorDisplayField>
        <errorMessage>You are not allowed to change the status manually.</errorMessage>
    </validationRules>
