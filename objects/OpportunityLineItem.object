    <validationRules>
        <fullName>CN_Prevent_Create_Edit</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
OR(
   ISPICKVAL(Opportunity.StageName, &quot;Closed Won&quot;),
ISPICKVAL(Opportunity.StageName, &quot;Closed Lost&quot;),
ISPICKVAL(Opportunity.StageName, &quot;Closed Cancelled&quot;)
)
)</errorConditionFormula>
        <errorMessage>It&apos;s not allow to create or edit Interested SKUs if opportunity had closed.</errorMessage>
    </validationRules>
