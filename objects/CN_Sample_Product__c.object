        <massQuickActions>CN_Update_Deliver_Status</massQuickActions>
        <massQuickActions>CN_Update_Customer_Feedback</massQuickActions>
        <searchResultsAdditionalFields>CN_Sample_Request_Type__c</searchResultsAdditionalFields>
        <searchResultsAdditionalFields>CN_Customer_Name__c</searchResultsAdditionalFields>
        <searchResultsAdditionalFields>CN_Product__c</searchResultsAdditionalFields>
        <searchResultsAdditionalFields>CN_Sample_SKU_Code__c</searchResultsAdditionalFields>
        <searchResultsAdditionalFields>CN_Sample_Unit_List_Price__c</searchResultsAdditionalFields>
        <searchResultsAdditionalFields>CREATED_DATE</searchResultsAdditionalFields>
        <searchResultsAdditionalFields>CN_Status_of_Delivery__c</searchResultsAdditionalFields>
    </searchLayouts>
    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>CN_Check_Deliver_Status_After_Done</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
CN_Is_Deliver_Done__c,
 CN_Is_Sample_Request_Approved__c ,
 ISCHANGED(CN_Status_of_Delivery__c ) 
)</errorConditionFormula>
        <errorDisplayField>CN_Status_of_Delivery__c</errorDisplayField>
        <errorMessage>It&apos;s not allowed to change deliver status after all sample product deliver status had been selected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Check_FeedBack</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
 NOT(ISPICKVAL(CN_Status_of_Delivery__c  , &quot;Done&quot;)),
 ISCHANGED( CN_Customer_Feedback__c ) || ISNEW(),
 NOT(ISPICKVAL(CN_Customer_Feedback__c , &quot;&quot;))
)</errorConditionFormula>
        <errorDisplayField>CN_Customer_Feedback__c</errorDisplayField>
        <errorMessage>Fill in customer feedback after sample deliveried.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Check_Feedback_Message</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  ISPICKVAL(CN_Customer_Feedback__c , &quot;&quot;) ,
   NOT(ISBLANK( CN_Feedback_Description__c ) )
)</errorConditionFormula>
        <errorDisplayField>CN_Feedback_Description__c</errorDisplayField>
        <errorMessage>Please select Customer Feedback</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Check_Product_Active</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
ISCHANGED( CN_Product__c ),
NOT( CN_Product__r.IsActive )
)</errorConditionFormula>
        <errorDisplayField>CN_Product__c</errorDisplayField>
        <errorMessage>Product is inactive</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Check_Product_Type</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
 CN_Is_Repack_Sample__c = true,
  CN_Product__r.Is_Repack_Sample__c = false
)</errorConditionFormula>
        <errorDisplayField>CN_Product__c</errorDisplayField>
        <errorMessage>This Product not support office pack</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Check_Repack_Deliver_Status_Edit</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
 OR(
   $Profile.Name = &quot;CN Sales&quot;,
   $Profile.Name = &quot;CN Sales Lead&quot;,
   $Profile.Name = &quot;CN Sales Manager&quot;
 ),
 CN_Is_Repack_Sample__c ,
 ISCHANGED(CN_Status_of_Delivery__c  ) ||  ISCHANGED( CN_Delivery_Comments__c ) 
)</errorConditionFormula>
        <errorDisplayField>CN_Status_of_Delivery__c</errorDisplayField>
        <errorMessage>Repack Sample&apos;s deliver status is only updated by CCE</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Check_Sample_Quantity</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
 CN_Quantity_Needed__c &lt;= 0
)</errorConditionFormula>
        <errorDisplayField>CN_Quantity_Needed__c</errorDisplayField>
        <errorMessage>Quantity Need must greater than 0.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Check_Wholepack_Deliver_Status_Edit</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
 NOT(OR(
   $Profile.Name = &quot;CN Sales&quot;,
   $Profile.Name = &quot;CN Sales Lead&quot;,
   $Profile.Name = &quot;CN Sales Manager&quot;
 )),
 NOT(CN_Is_Repack_Sample__c ),
 ISCHANGED(CN_Status_of_Delivery__c  ) ||  ISCHANGED( CN_Delivery_Comments__c ) 
)</errorConditionFormula>
        <errorDisplayField>CN_Status_of_Delivery__c</errorDisplayField>
        <errorMessage>Wholepack Sample&apos;s deliver status is only updated by Sales Person</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Prevent_Create</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
ISNEW(),
 NOT(CN_Sample_Request__r.CN_Is_Draft__c )
)</errorConditionFormula>
        <errorMessage>It&apos;s not allowed to create new sample product after sample request ready approve or approved.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Prevent_Edit_After_Ready</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
NOT(CN_Sample_Request__r.CN_Is_Draft__c), 
NOT(ISCHANGED(CN_Customer_Feedback__c )),
NOT(ISCHANGED( CN_Feedback_Description__c )),
NOT(ISCHANGED(  CN_Feedback_Date__c )),
NOT(ISCHANGED(   CN_Delivery_Comments__c )),
NOT(ISCHANGED( CN_Status_of_Delivery__c ))
)</errorConditionFormula>
        <errorMessage>It&apos;s not allow to edit when ready for approved, approved or canceled.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_SP_Check_Current_Status_Delivery</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  NOT(ISBLANK(  TEXT(CN_Current_Status__c) )),
  ISBLANK(TEXT(CN_Status_of_Delivery__c)) 
)</errorConditionFormula>
        <errorDisplayField>CN_Current_Status__c</errorDisplayField>
        <errorMessage>You must enter the delivery status before you enter the current status.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_SP_Check_Current_Status_Sales</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  ISCHANGED(  CN_Current_Status__c ),
  NOT(OR(
    $Profile.Name = &quot;CN Sales&quot;,
    $Profile.Name = &quot;CN Sales Lead&quot;,
    $Profile.Name = &quot;CN Sales Manager&quot;
  )) 
)</errorConditionFormula>
        <errorDisplayField>CN_Current_Status__c</errorDisplayField>
        <errorMessage>You are not allowed to edit the current status.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
