    <fields>
        <fullName>CN_Is_Overdue__c</fullName>
        <externalId>false</externalId>
        <formula>AND(
    CN_Is_Sample_Request_Approved__c,
    ISPICKVAL(CN_Status_of_Delivery__c, &quot;Done&quot;),
    IF( 
        CN_Current_Status_Date__c = null &amp;&amp;  CN_Feedback_Date__c = null,  
        (TODAY() -  CN_Sample_Request__r.CN_Sample_Needed_Date__c) &gt; 14, 
        IF(
             CN_Current_Status_Date__c = null, 
            (CN_Feedback_Date__c -  CN_Sample_Request__r.CN_Sample_Needed_Date__c) &gt; 14 ,
            (CN_Current_Status_Date__c -  CN_Sample_Request__r.CN_Sample_Needed_Date__c) &gt; 14
         )
    )
)</formula>
        <label>Is Overdue</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
